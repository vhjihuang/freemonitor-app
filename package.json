{
  "name": "freemonitor-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "kill-port 3001 && turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:api": "cd apps/backend && npm run docs:generate",
    "docs:serve": "cd apps/backend && npm run docs:serve",
    "docs:all": "node scripts/update-all-docs.cjs",
    "clean": "rimraf node_modules pnpm-lock.yaml && pnpm -r exec -- rimraf node_modules || true"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.19.11",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.34.0",
    "kill-port": "^2.0.1",
    "rimraf": "^5.0.0",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  },
  "pnpm": {
    "overrides": {
      "@nestjs/common": "^10.0.0",
      "@nestjs/core": "^10.0.0",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "@types/react": "^18.3.1",
      "@types/react-dom": "^18.3.1"
    }
  },
  "dependencies": {
    "critters": "^0.0.25",
    "glob": "^10.3.10"
  }
}