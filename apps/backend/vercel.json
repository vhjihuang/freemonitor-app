{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/src/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/src/main.js"
    }
  ],
  "functions": {
    "dist/src/main.js": {
      "maxDuration": 30
    }
  }
}